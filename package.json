    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": ["lib/**/*"],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "Massinissa Hamidouche",
  "license": "ISC",
  "devDependencies": {
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  }
